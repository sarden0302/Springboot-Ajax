<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kh.AjaxSpringProject.mapper.PersonMapper">
    <select id="getAllPerson">
        SELECT * FROM users
    </select>

    <select id="getPersonById">
        SELECT * FROM users
        WHERE id = #{id}
    </select>

</mapper>